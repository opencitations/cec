<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<tox-type nite:id="t_1" type="toxicity" xmlns:nite="http://nite.sourceforge.net/">
   <!-- e.g. things such as plutonium or sulfuric acid -->
   <tox-type nite:id="t_2" type="dangerous">
      <tox-type nite:id="t_3" type="radioactive"/>
      <tox-type nite:id="t_4" type="highly-reactive"/>
   </tox-type>
   <!-- e.g. things such as mineral oils -->
   <tox-type nite:id="t_5" type="toxic">
      <tox-type nite:id="t_6" type="letal-in-small-quantities">
         <tox-type nite:id="t_7" type="to-ingestion"/>
         <tox-type nite:id="t_8" type="to-inhalation"/>
      </tox-type>
      <tox-type nite:id="t_9" type="pollutant">
         <tox-type nite:id="t_10" type="diffusion">
            <tox-type nite:id="t_11" type="to-water"/>
            <tox-type nite:id="t_12" type="to-air"/>
            <tox-type nite:id="t_13" type="to-soil"/>
         </tox-type>
         <tox-type nite:id="t_14" type="exposition">
            <tox-type nite:id="t_15" type="to-man"/>
            <tox-type nite:id="t_16" type="to-animals"/>
         </tox-type>
      </tox-type>
   </tox-type>
   <!-- e.g. things such as ammonia or soda -->
   <tox-type nite:id="t_17" type="mild-toxicity">
      <tox-type nite:id="t_18" type="acid"/>
      <tox-type nite:id="e_19" type="basic"/>
   </tox-type>
   <!-- e.g. water -->
</tox-type>